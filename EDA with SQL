import sqlite3

conn = sqlite3.connect('spacex.db')  
cursor = conn.cursor()               

cursor.execute("SELECT COUNT(*) FROM spacex_launch_data")
print(cursor.fetchone()[0])  

cursor = conn.cursor()

# Launches by Year
cursor.execute("""
    SELECT strftime('%Y', launch_date) AS year, COUNT(*) 
    FROM spacex_launch_data 
    GROUP BY year 
    ORDER BY year;
""")
print(cursor.fetchall())
